"Host *.acme.com\n"
"    User jdoe\n"
"    Compression yes # default is no\n"
"    PasswordAuthentication no # only use public key\n"
"    ServerAliveInternal 60 # keep-alives for bad firewall\n"
"\n"
msgstr ""

#. type: lstlisting
#: ssh.tex:288
#, no-wrap
msgid ""
"Host *\n"
"    User john"
msgstr ""

#. type: itemize
#: ssh.tex:293
msgid ""
"Note: \\texttt{bash-completion} can auto-complete using \\texttt{ssh"
"\\_config} hosts"
msgstr ""
"Note: \\texttt{bash-completion} peut auto-compléter en utilisant les hôtes "
"de  \\texttt{ssh\\_config}"

#. type: section{#2}
#: ssh.tex:295
msgid "Advanced usage"
msgstr "Utilisation avancée"

#. type: frame{#2}
#: ssh.tex:308
msgid "SSH as a communication layer for applications"
msgstr "SSH : couche de communication pour applications"

#. type: itemize
#: ssh.tex:308
#, fuzzy
#| msgid "Several applications use SSH as their communication layer\\\\"
msgid ""
"Several applications use SSH as their communication (and authentication) "
"layer"
msgstr "Plusieurs applications utilisent SSH comme leur couche communication "

#. type: itemize
#: ssh.tex:308
msgid "\\texttt{scp}, \\texttt{sftp}, \\texttt{rsync} (data transfer)"
msgstr "\\texttt{scp}, \\texttt{sftp}, \\texttt{rsync} (transfert de donnée)"

#. type: itemize
#: ssh.tex:308
msgid "\\texttt{lftp} (CLI) and \\texttt{gftp} (GUI) support the SFTP protocol"
msgstr ""

#. type: itemize
#: ssh.tex:308
msgid "\\texttt{unison} (synchronization)"
msgstr "\\texttt{unison} (synchronisation)"

#. type: itemize
#: ssh.tex:315
msgid ""
"\\alert{Subversion:} \\texttt{svn checkout svn+ssh://user@rhost/path/to/repo}"
msgstr ""

#. type: itemize
#: ssh.tex:315
msgid ""
"\\alert{Git:} \\texttt{git clone ssh://git@github.com/path-to/repository."
"git}\\\\ Or: \\texttt{git clone git@github.com:path-to/repository.git}"
msgstr ""
"\\alert{Git:} \\texttt{git clone ssh://git@github.com/path-to/repository."
"git}\\\\ Ou: \\texttt{git clone git@github.com:path-to/repository.git}"

#. type: frame{#2}
#: ssh.tex:333
msgid "Access remote filesystems over SSH: sshfs"
msgstr "Accès à un filesystem distant à travers SSH: sshfs"

#. type: itemize
#: ssh.tex:333
msgid "\\texttt{sshfs}: FUSE-based solution to access remote machines"
msgstr ""
"\\texttt{sshfs}: Des solution basées sur FUSE pour accéder à des machines "
"distantes"

#. type: itemize
#: ssh.tex:333
msgid ""
"Ideal for remote file editing with a GUI, copying small amounts of data, etc."
msgstr ""
